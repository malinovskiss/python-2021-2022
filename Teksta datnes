#1
file = open('otrais.txt','r', encoding='utf-8')
print(file.read())
file.close()

#2
file2 = open("otrais.txt",'r', encoding="utf-8")
print("simbolu skaits:",len(file2.read()))
file2.close()

#3
file3 = open('otrais.txt','r', encoding='utf-8')
print("pirmie 10 simboli:",file3.read(10))
file3.close()
